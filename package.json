    "rxjs": "5.4.3",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.0.0",
    "@types/jasmine": "^2.6.2",
    "@types/node": "^8.0.44",
    "angular-cli": "^1.0.0-beta.28.3",
    "angular-mocks": "^1.6.6",
    "angular2-template-loader": "^0.6.2",
    "jasmine": "^2.8.0",
    "jasmine-core": "^2.8.0",
    "jasmine-ts": "^0.2.1",
